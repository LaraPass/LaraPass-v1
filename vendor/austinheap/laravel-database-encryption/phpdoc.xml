<?xml version="1.0" encoding="UTF-8"?>
<phpdocumentor>
    <title>laravel-database-encryption docs</title>
    <parser>
        <target>build/docs</target>
        <encoding>utf8</encoding>
        <markers>
            <item>TODO</item>
        </markers>
        <visibility></visibility>
        <extensions>
            <extension>php</extension>
        </extensions>
    </parser>
    <transformer>
        <target>docs</target>
    </transformer>
    <transformations>
        <template name="checkstyle" />
        <template name="clean" />
    </transformations>
    <files>
        <directory>src</directory>
        <ignore>vendor/*</ignore>
        <ignore>tests/*</ignore>
    </files>
    <logging>
        <level>err</level>
        <paths>
            <default>build/log/{DATE}.log</default>
            <errors>build/log/{DATE}.errors.log</errors>
        </paths>
    </logging>
</phpdocumentor>
